<!DOCTYPE html>
<html>
<head>
  <title>Brainforest</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id = "header">amnotazon</div>
<% if flash[:notice] %>
	<p id = "notice">
		<%= flash[:notice] %>
	</p>
<% end %>
<% if flash[:alert] %>
	<p id = "alert">
		<%= flash[:alert] %>
	</p>
<% end %>
<%= yield %>

</body>
</html>
